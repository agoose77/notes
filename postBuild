#!/bin/bash
jupyter labextension install @jupyterlab/katex-extension #@agoose77/jupyterlab-markup
#echo "application/wasm      wasm" | sudo tee -a /etc/mime.types
ws_path=$(jupyter lab path | sed "s/Workspaces directory: (.*)/\1/p"-En)
cp "labworkspacesmain-workspace-db39.jupyterlab-workspace" "$ws_path"